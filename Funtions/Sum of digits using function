//Sum of digits using function
#include <stdio.h>
int getsum(int n){
    int l,sum=0;
    while(n>0){
       l=n%10;
       sum=sum+l;
       n=n/10;
   }
   return sum;
}
int main(){
    int n,d;
    printf("Enter a number:");
    scanf("%d", &n);
    d=getsum(n);
    printf("%d",d);
    return 0;
}
